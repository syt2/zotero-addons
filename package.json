{
  "name": "zotero-addons",
  "version": "1.6.10",
  "config": {
    "addonName": "Add-on Market for Zotero",
    "addonID": "zoteroAddons@ytshen.com",
    "addonRef": "zoteroaddons",
    "addonInstance": "ZoteroAddons",
    "prefsPrefix": "extensions.zotero.zoteroaddons"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syt2/zotero-addons.git"
  },
  "author": "ytshen",
  "bugs": {
    "url": "https://github.com/syt2/zotero-addons/issues"
  },
  "homepage": "https://github.com/syt2/zotero-addons#readme",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "start": "zotero-plugin serve",
    "build": "tsc --noEmit && zotero-plugin build",
    "lint": "prettier --write . && eslint . --fix",
    "release": "bumpp --commit \"chore(release): publish V%s\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update-deps": "npm update --save"
  },
  "dependencies": {
    "zotero-plugin-toolkit": "^2.3.31"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@types/node": "^20.12.12",
    "eslint": "^9.2.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.9.0",
    "zotero-plugin-scaffold": "^0.0.23",
    "zotero-types": "^2.0.0"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "endOfLine": "lf",
    "overrides": [
      {
        "files": [
          "*.xhtml"
        ],
        "options": {
          "htmlWhitespaceSensitivity": "css"
        }
      }
    ]
  }
}
