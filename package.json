{
  "name": "zotero-addons",
  "version": "1.6.4",
  "config": {
    "addonName": "Add-on Market for Zotero",
    "addonID": "zoteroAddons@ytshen.com",
    "addonRef": "zoteroaddons",
    "addonInstance": "ZoteroAddons",
    "prefsPrefix": "extensions.zotero.zoteroaddons"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syt2/zotero-addons.git"
  },
  "author": "ytshen",
  "bugs": {
    "url": "https://github.com/syt2/zotero-addons/issues"
  },
  "homepage": "https://github.com/syt2/zotero-addons#readme",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "start": "zotero-plugin serve",
    "build": "tsc --noEmit && zotero-plugin build",
    "lint": "prettier --write . && eslint . --fix",
    "release": "zotero-plugin release",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update-deps": "npm update --save"
  },
  "dependencies": {
    "zotero-plugin-toolkit": "^2.3.36"
  },
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@types/node": "^20.14.10",
    "eslint": "^9.6.0",
    "prettier": "^3.3.2",
    "typescript": "^5.5.2",
    "typescript-eslint": "^8.0.0-alpha.37",
    "zotero-plugin-scaffold": "^0.0.32",
    "zotero-types": "^2.0.2"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "endOfLine": "lf",
    "overrides": [
      {
        "files": [
          "*.xhtml"
        ],
        "options": {
          "htmlWhitespaceSensitivity": "css"
        }
      }
    ]
  }
}